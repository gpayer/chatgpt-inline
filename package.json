{
  "name": "chatgpt-inline",
  "displayName": "ChatGPT Inline",
  "description": "Provide inline completion support through ChatGPT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "chatgpt-inline.notifier-test",
        "title": "ChatGPT Inline: Test Notifier"
      }
    ],
    "configuration": [
      {
        "title": "ChatGPT Inline",
        "properties": {
          "chatgptInline.apiKey": {
            "type": "string",
            "default": "",
            "description": "OpenAI API Key"
          },
          "chatgptInline.model": {
            "type": "string",
            "default": "gpt-3.5-turbo",
            "enumItemLabels": [
              "GPT-3.5 Turbo",
              "GPT-4",
              "Davinci",
              "Ada",
              "Babbage",
              "Curie",
              "Davinci-Instruct-beta",
              "Davinci-codex",
              "Davinci-codex-beta",
              "Davinci-instruct-beta",
              "Davinci-beta",
              "Ada-beta",
              "Babbage-beta",
              "Curie-beta"
            ],
            "enum": [
              "gpt-3.5-turbo",
              "gpt-4",
              "davinci",
              "ada",
              "babbage",
              "curie",
              "davinci-instruct-beta",
              "davinci-codex",
              "davinci-codex-beta",
              "davinci-instruct-beta",
              "davinci-beta",
              "ada-beta",
              "babbage-beta",
              "curie-beta"
            ]
          },
          "chatgptInline.notifier": {
            "type": "string",
            "default": "none",
            "enum": [
              "none",
              "test"
            ],
            "enumDescriptions": [
              "No notifier",
              "Test notifier"
            ],
            "description": "Notifier to use"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.6"
  }
}
